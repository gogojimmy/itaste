<div id="fb-root"></div>
<script>(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = "//connect.facebook.net/zh_TW/all.js#xfbml=1&appId=<%= OMNIAUTH_CONFIG['facebook']['app_id'] %>";
  fjs.parentNode.insertBefore(js, fjs);
  }(document, 'script', 'facebook-jssdk'));</script>
<div class="fb-comments" data-href="<%= "#{request.protocol}#{request.host_with_port}#{request.fullpath}" %>" data-num-posts="5" data-colorscheme="dark"></div>
